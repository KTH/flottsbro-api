{
  "//": "The production flag is added in the jenkins build script.",
  "name": "flottsbro-api",
  "version": "0.1.0",
  "description": "Restful API for getting information about applications deployed in Docker clusters. .",
  "main": "app.js",
  "private": true,
  "scripts": {
    "codecheck": "prettier",
    "start": "NODE_ENV=production node app.js",
    "start-dev": "LOGGING_LEVEL=info NODE_ENV=development nodemon app.js",
    "clean": "rm -rf ./node_modules && rm -f package-lock.json",
    "test": "",
    "test-integration": "./test/integration/basic.sh",
    "docker-build-local": "source docker.conf && docker build --tag $IMAGE_NAME:$IMAGE_VERSION .",
    "package-update": "git pull && npm run clean & ncu -u && npm install && npm audit fix && npm test"
  },
  "dependencies": {
    "@kth/in-memory-cache": "^0.1.19",
    "@slack/webhook": "^6.0.0",
    "applicationinsights": "^1.8.9",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "co": "^4.6.0",
    "component-registry": "^1.3.1",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-handlebars": "^5.2.0",
    "gulp": "^4.0.2",
    "kth-node-access-log": "^0.2.2",
    "kth-node-api-common": "^1.0.1",
    "kth-node-api-key-strategy": "^1.0.9",
    "kth-node-configuration": "^2.0.3",
    "kth-node-express-routing": "^2.0.3",
    "kth-node-log": "^3.1.3",
    "kth-node-mongo": "3.3.7",
    "kth-node-monitor": "^1.1.8",
    "kth-node-response": "KTH/kth-node-response.git#v1.0.0",
    "kth-node-server": "^3.1.2",
    "mongoose": "^5.11.14",
    "passport": "^0.4.1",
    "safe-utils": "^0.2.1",
    "swagger-ui": "^3.41.1"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "mocha": "^8.2.1",
    "nodemon": "^2.0.7",
    "webpack": "^5.20.1"
  },
  "engines": {
    "node": "12"
  }
}
