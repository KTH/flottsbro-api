{
  "//": "The production flag is added in the jenkins build script.",
  "name": "flottsbro-api",
  "version": "0.1.0",
  "description": "Restful API for getting information about applications deployed in Docker clusters. .",
  "main": "app.js",
  "private": true,
  "scripts": {
    "codecheck": "prettier",
    "start": "NODE_ENV=production node app.js",
    "start-dev": "LOGGING_LEVEL=info NODE_ENV=development nodemon app.js",
    "test": "mocha test/**/*.js",
    "test-unit-in-docker": "ID=$(docker build --pull -q .) && LOCAL_IMAGE_ID=$ID docker-compose -f docker-compose-unit-tests.yml up --abort-on-container-exit --always-recreate-deps"
  },
  "dependencies": {
    "@slack/webhook": "^5.0.2",
    "applicationinsights": "^1.6.0",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "co": "^4.6.0",
    "component-registry": "^1.1.0",
    "cookie-parser": "^1.4.4",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-handlebars": "^3.1.0",
    "gulp": "^4.0.2",
    "kth-node-access-log": "^0.2.0",
    "kth-node-api-common": "^1.0.1",
    "kth-node-api-key-strategy": "^1.0.8",
    "kth-node-configuration": "^2.0.1",
    "kth-node-express-routing": "^2.0.3",
    "kth-node-log": "^3.0.2",
    "kth-node-mongo": "3.1.2",
    "kth-node-monitor": "^1.0.26",
    "kth-node-response": "KTH/kth-node-response.git#v1.0.0",
    "kth-node-server": "^3.1.2",
    "mongoose": "^5.7.14",
    "passport": "^0.4.0",
    "safe-utils": "^0.2.1",
    "swagger-ui": "^3.24.3"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "concurrently": "^5.0.0",
    "cross-env": "^6.0.3",
    "husky": "^3.1.0",
    "istanbul": "^0.4.5",
    "mocha": "^6.2.2",
    "nodemon": "^2.0.1",
    "proxyquire": "^2.1.3",
    "tap-spec": "^5.0.0",
    "tape": "^4.11.0",
    "webpack": "^4.41.2"
  },
  "engines": {
    "node": "8.9"
  }
}
